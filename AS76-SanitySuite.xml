            </methods>
            <class name="RBC.VerifySubTabOfRBC.VerifyUiOfRbcShapeSubTabTest"/>
            <methods>
                <include name="navigateToShapeTab"/>
                <include name="columnOnShapeSubTab"/>
                <include name="cellNameOnShapeSubTab"/>
